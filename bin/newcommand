# Create a new command from templatesh.sh

echo "New Command from template.sh"

# get first arg

NEWCOMMAND=$1

# make the command directory

mkdir $NEWCOMMAND

# copy the template to new directory

cp newcommand/templatesh.sh $NEWCOMMAND/$NEWCOMMAND.sh

# add command to the console

ln $NEWCOMMAND/$NEWCOMMAND.sh bin/$NEWCOMMAND

